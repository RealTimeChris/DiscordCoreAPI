@PACKAGE_INIT@

set_and_check(EXPORT_TARGETS_FILE_NEW "@PACKAGE_EXPORTED_TARGETS_FILEPATH@")

include("${EXPORT_TARGETS_FILE_NEW}")

if (WIN32)
	if (EXISTS "@PACKAGE_RELEASE_PDB_FILEPATH@")
		set_and_check(FILE_PATH_RELEASE "@PACKAGE_RELEASE_PDB_FILEPATH@")
		cmake_path(GET FILE_PATH_RELEASE PARENT_PATH RELEASE_PDB_FILE_PATH_NEW)
		set_and_check(RELEASE_PDB_FILE_PATH "${FILE_PATH_RELEASE}")
		cmake_path(GET FILE_PATH_RELEASE FILENAME RELEASE_PDB_FILE_NAME_NEW)
		set(RELEASE_PDB_FILE_NAME "${RELEASE_PDB_FILE_NAME_NEW}")
	endif()	
	if (EXISTS "@PACKAGE_DEBUG_PDB_FILEPATH@")
		set_and_check(FILE_PATH_DEBUG "@PACKAGE_DEBUG_PDB_FILEPATH@")
		cmake_path(GET FILE_PATH_DEBUG PARENT_PATH DEBUG_PDB_FILE_PATH_NEW)
		set_and_check(DEBUG_PDB_FILE_PATH "${FILE_PATH_DEBUG}")
		cmake_path(GET FILE_PATH_DEBUG FILENAME DEBUG_PDB_FILE_NAME_NEW)
		set(DEBUG_PDB_FILE_NAME "${DEBUG_PDB_FILE_NAME_NEW}")
	endif()	
endif()

check_required_components(DiscordCoreAPI_CMake)
