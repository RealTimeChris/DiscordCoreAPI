@PACKAGE_INIT@ 
include(CMakeFindDependencyMacro)
@DiscordCoreAPI_FIND_DEPENDENCY_CALLS@
set_and_check(DISCORDCOREAPI_HEADER_DIR "@PACKAGE_DISCORDCOREAPI_INCLUDE_DIR@")
set_and_check(DISCORDCOREAPI_SOURCE_DIR "@PACKAGE_DISCORDCOREAPI_SOURCE_DIR@")
include ( ${CMAKE_CURRENT_LIST_DIR}/DiscordCoreAPITargets.cmake )
set(LIBRARY_NAMES_NEW "@LIBRARY_NAMES@")
set(RELEASE_LIBRARIES_NEW "@RELEASE_LIBRARIES@")
set(DEBUG_LIBRARIES_NEW "@DEBUG_LIBRARIES@")
set(RELEASE_DLLS_NEW "@RELEASE_DLLS@")
set(DEBUG_DLLS_NEW "@DEBUG_DLLS@")

foreach(LIBRARY_NAME ${LIBRARY_NAMES_NEW})
	list(FIND LIBRARY_NAMES_NEW "${LIBRARY_NAME}" THE_INDEX)
	list(GET RELEASE_LIBRARIES_NEW "${THE_INDEX}" RELEASE_LIBRARY)
	list(GET DEBUG_LIBRARIES_NEW "${THE_INDEX}" DEBUG_LIBRARY)
	list(GET RELEASE_DLLS_NEW "${THE_INDEX}" RELEASE_DLL)
	list(GET DEBUG_DLLS_NEW "${THE_INDEX}" DEBUG_DLL)
	if (RELEASE_LIBRARY AND DEBUG_LIBRARY)
		set_and_check(DEBUG_LIBRARY_NEW "${DEBUG_LIBRARY}")
		set_and_check(RELEASE_LIBRARY_NEW "${RELEASE_LIBRARY}")
		add_library("${LIBRARY_NAME}" STATIC IMPORTED)
		set_target_properties("${LIBRARY_NAME}" PROPERTIES IMPORTED_LOCATION_RELEASE "${RELEASE_LIBRARY_NEW}" IMPORTED_LOCATION_DEBUG "${DEBUG_LIBRARY_NEW}")
		if (RELEASE_DLL AND DEBUG_DLL)
			add_library("${LIBRARY_NAME}_SHARED" SHARED IMPORTED)
			set_and_check(DEBUG_DLL_NEW "${DEBUG_DLL}")
			set_and_check(RELEASE_DLL_NEW "${RELEASE_DLL}")
			set_target_properties("${LIBRARY_NAME}_SHARED" PROPERTIES IMPORTED_LOCATION_RELEASE "${RELEASE_DLL_NEW}" IMPORTED_LOCATION_DEBUG "${DEBUG_DLL_NEW}" IMPORTED_IMPLIB_RELEASE "${RELEASE_LIBRARY_NEW}" IMPORTED_IMPLIB_DEBUG "${DEBUG_LIBRARY_NEW}")
			target_link_libraries(DiscordCoreAPI::DiscordCoreAPI INTERFACE "${LIBRARY_NAME}_SHARED")
		else()
			target_link_libraries(DiscordCoreAPI::DiscordCoreAPI INTERFACE "${LIBRARY_NAME}")
		endif()
	endif()	
endforeach()

check_required_components(DiscordCoreAPI)